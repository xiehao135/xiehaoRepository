/* pages/order/order.wxss */

.table {
  font-size: 28rpx;
  font-weight: bold;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}

.th {
  width: 40%;
  justify-content: left;
  background: rgb(231, 225, 225);
  color: rgb(93, 89, 89);
  display: flex;
  height: 3rem;
  align-items: center;
}

.pic-view{
  width:80%;
  height:200rpx;
  background: rgb(231, 225, 225);
  display: flex;
  justify-content: center;
  padding-bottom: 20rpx;
  margin:0 auto;
}

.pic{
  width: 300rpx;
  height:200rpx;
}
