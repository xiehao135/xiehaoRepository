/* pages/me/administrator/administrator.wxss */
.h2{
  width: 100%;
  font-size: 48rpx;
  float: left;
  font-weight: bold;
  line-height: 58rpx;
  margin-top: 30rpx;
}

input{
  background-color: rgba(239, 239, 239, 0.7);
  border-radius: 40rpx;
  margin-top: 32rpx;
  width: 90%;
  float: left;
  height: 112rpx;
  font-size: 30rpx;
  padding:0 5%;
}

button{
  color: white;
  border-radius: 40rpx;
  margin-top: 50rpx;
  width: 100%;
  float: left;
  height: 112rpx;
  font-size: 30rpx;
  line-height: 112rpx;
  border: solid 1px #5abd8c;
  background-color: #5abd8c;
  box-shadow: 0px 15px 20px 0px rgba(90, 189, 140, 0.55);
}


.but1{
  background-image: linear-gradient(313deg, #5abd8c 0%, #00ff81 100%);
  box-shadow: 0px 5px 15px 0px rgba(90, 189, 140, 0.5);
  border-radius: 20rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  letter-spacing: 0px;
  color: #ffffff;
  width: 40%;
  height: 60rpx;
}

.image_v{
  background-color: #ffffff;
  border-radius: 6px;
  border: solid 1px #707070;
  height: 320rpx;
  width: 100%;
  float: left;
  margin-top: 50rpx;
}

.image_v image{
  height: 100%;
  width: 100%;
  float: center;
}

.image_v text{
  height: 100%;
  width: 100%;
  float: left;
  text-align: center;
  line-height: 320rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #000000;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.content{
  position: absolute;
  width: 90%;
  height: 100%;
  left: 5%;
  top: 0;
}

.back_img{
  width: 100%;
  float: left;
}
button::after{ border: none; }